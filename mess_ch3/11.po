msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Strong]\n"
"Ugh... Errrggg...!!\n"
"What is this card!?\n"
"[NEW_DIALOG]\n"
"My EM wave patterns are\n"
"being rewritten...!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"This is a card that makes\n"
"Wizards go out of control...\n"
"[NEW_DIALOG]\n"
"I'm giving you the title of\n"
"Club.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "2"
msgid ""
"{e1a0}{4}x{ec00}{0}\n"
"[DIALOG_START][Strong]\n"
"Ga{e114}a{e114}a{e114}a{e114}a{e114}h!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "3"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Club_Strong]\n"
"Ha{e113}.{e113}.{e113}.{e113}aa{e113}.{e113}.{e113}.{e113}\n"
"...Aaahhh!\n"
"[NEW_DIALOG]\n"
"The power is unbelievable!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"...You have been reborn.\n"
"You are now known as...\n"
"[NEW_DIALOG]\n"
"Club Strong.\n"
"[NEW_DIALOG]\n"
"You'll never work for the\n"
"good of humans, ever again...\n"
"[NEW_DIALOG]\n"
"You're going to use your\n"
"new powers and abilities to\n"
"control this island...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Club_Strong]\n"
"Yes I will...!\n"
"[NEW_DIALOG]\n"
"My power is much too great\n"
"for pathetic humans to have\n"
"any control over!!\n"
"[NEW_DIALOG]\n"
"I shall become the new king\n"
"of {e4c4}!\n"
"[NEW_DIALOG]\n"
"My first action as king will\n"
"be to use the environmental\n"
"system as a weapon!\n"
"[NEW_DIALOG]\n"
"Ho ho ho ho ho!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Jack]\n"
"{ec13}...\n"
"[NEW_DIALOG]\n"
"{ec13}(He's so strong, now...)\n"
"[NEW_DIALOG]\n"
"{ec13}(Geo Stelar...\n"
" No, Mega Man...\n"
" This is the end...)\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"You look a little down, Jack...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Jack]\n"
"{ec13}!!\n"
"[NEW_DIALOG]\n"
"Whatever...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "5"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][(Queen)Tia]\n"
"...So what Mr. King said\n"
"wasn't exactly off the mark,\n"
"was it...?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "6"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Jack]\n"
"W-What do you mean?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"You've gotten too close to\n"
"Geo Stelar and his friends.\n"
"It's made you weak...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Jack]\n"
"{ec13}!!\n"
"[NEW_DIALOG]\n"
"Give me a break!!\n"
"I'm going to take Mega Man\n"
"out...\n"
"[NEW_DIALOG]\n"
"And then I'll make our wish\n"
"come true!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"{ec13}...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Jack]\n"
"Don't worry, Sis!\n"
"You can count on me!!\n"
"[NEW_DIALOG]\n"
"I couldn't care less about\n"
"the whole lot of 'em!\n"
"[NEW_DIALOG]\n"
"If you want, I'll show you\n"
"and take him out, right now!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"...That could be interesting...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
