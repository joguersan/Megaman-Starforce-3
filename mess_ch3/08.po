msgctxt "12"
msgid ""
"{ec00}{0}{e213}{0}\n"
"[DIALOG_START][Jack_Baño]\n"
"What a jolly bunch...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"{ec13}...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{e213}{0}\n"
"[DIALOG_START][Jack_Baño]\n"
"Míralos, qué contentos\n"
"todos...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"{ec13}...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "13"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Jack_Baño]\n"
"Sis?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"Strong...\n"
"He's more powerful than\n"
"I had expected...\n"
"[NEW_DIALOG]\n"
"This should be pretty fun...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Jack_Baño]\n"
"¿Tia?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][(Queen)Tia]\n"
"Strong...\n"
"Es más poderoso de\n"
"lo que esperaba...\n"
"[NEW_DIALOG]\n"
"Qué divertido va\n"
"a ser esto...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
