msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Megaman]\n"
"Come on!\n"
"[NEW_DIALOG]\n"
"Give me back all of Prez,\n"
"Sonia, and Bud's stuff!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{97}You really think I'm gonna\n"
"listen to you!?\n"
"[NEW_DIALOG]\n"
"A real thief would never give\n"
"back anything he took!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Megaman]\n"
"Then I'll have to take it all\n"
"back from you!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{97}Go ahead and try!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e212}{19}{0}"
msgstr ""

msgctxt "2"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START]{97}Arrrgh!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Megaman]\n"
"Are you ready to give up!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "3"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START]{97}Darn it!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"We'll get blown over if we\n"
"don't get out of here, kid!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Megaman]\n"
"I won't let that happen!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Megaman]\n"
"I can do this!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "5"
msgid ""
"{e213}{1}{ec00}{0}\n"
"[DIALOG_START][Megaman]\n"
"Ah!\n"
"I only have time to grab\n"
"one item!\n"
"[NEW_DIALOG]\n"
"Which one should I grab...!?\n"
"[NEW_DIALOG]\n"
"{ec05}{0}{0}   {ec06}Prez's handkerchief\n"
"   {ec09}Sonia's bag\n"
"   {ec0c}Bud's shirt\n"
"[NEW_DIALOG]\n"
"{ec11}{0}{8}{ec11}{3}{9}{ec11}{6}\n"
"{ec00}{1}"
msgstr ""

