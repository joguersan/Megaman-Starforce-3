msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][NONAME]\n"
"Two Weeks Later...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "2"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"Dad...\n"
"Mega...\n"
"[NEW_DIALOG]\n"
"I was able to make it home\n"
"safely thanks to you.\n"
"[NEW_DIALOG]\n"
"I've been having a great\n"
"time with everyone.\n"
"[NEW_DIALOG]\n"
"And I've decided to live my\n"
"life to its fullest, even if\n"
"you're not here...\n"
"[NEW_DIALOG]\n"
"But every once in a while I\n"
"get pretty sad...\n"
"[NEW_DIALOG]\n"
".{e113}.{e113}.{e113}I miss both of you so much.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "3"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"Huh?{e116}\n"
"What's with that star{e115}.{e115}.{e115}.{e115}?\n"
"[NEW_DIALOG]\n"
"Is it a blue shooting star{e115}.{e115}.{e115}.{e115}?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"It's...\n"
"[NEW_DIALOG]\n"
"It's getting closer!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "5"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"Aaahhh!!\n"
"It's coming this way!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "6"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
".{e115}.{e115}.{e115}Huh!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"Heh heh heh...\n"
"[NEW_DIALOG]\n"
"Hey, kid!!\n"
"I'm back!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
"Me.{e114}.{e114}.{e114}ga{e114}.{e114}.{e114}..!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "7"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"MEGAAA!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e212}{1d}{1}"
msgstr ""

msgctxt "8"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"This isn't a dream, right!?\n"
"[NEW_DIALOG]\n"
"You're really back!?\n"
"[NEW_DIALOG]\n"
"I thought I'd never see you\n"
"again!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"Heh heh heh!!\n"
"[NEW_DIALOG]\n"
"Did you think that I would\n"
"kick the bucket so easily!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "9"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"Oh yeah...\n"
"Someone else should be\n"
"here any minute now.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
".{e114}.{e114}.{e114}What!?\n"
"[NEW_DIALOG]\n"
"Y-You mean{e114}.{e114}.{e114}.{e114}\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "10"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"Ah, there he is!!\n"
"[NEW_DIALOG]\n"
"You might want to put your\n"
"baseball glove on, kid!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
"What!?\n"
"A-Again!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e213}{0}"
msgstr ""

msgctxt "11"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"W-W-Whoooaaa!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
