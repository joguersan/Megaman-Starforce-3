msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START]{3c}Let's show Dealer what we\n"
"at {e4c0} are really\n"
"made of!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{3d}If I rework the functions of\n"
"our transmission satellite, it\n"
"can show an image of space.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{3c}Nice!!\n"
"We can search for the boy\n"
"with that!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{9a}There's just one more\n"
"important thing to do then!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START]{3c}¡¡Ahora verá Dealer de lo\n"
"que es capaz el\n"
"{e4c0}!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{3d}Si reconfiguro las funciones\n"
"de nuestro satélite, nos\n"
"mostrará imágenes del espacio.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{3c}¡¡Bien!!\n"
"¡¡Así podremos buscar\n"
"al chico!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{9a}¡Entonces ahora solo\n"
"nos queda una cosa!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "2"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START]{9a}There is something that I\n"
"want to propose to the\n"
"whole world via television!\n"
"[NEW_DIALOG]\n"
"Belle, would you please\n"
"present my proposal to\n"
"everyone for me!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Belle]\n"
"O-Of course!!\n"
"I'm all over it!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START]{9a}¡Me gustaría proponer\n"
"una cosa al mundo entero\n"
"por televisión!\n"
"[NEW_DIALOG]\n"
"¡Belle, ¿te importaría hacer\n"
"llegar a todo el mundo\n"
"mi propuesta?!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Belle]\n"
"¡¡P-pues claro!!\n"
"¡¡Encantada!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
