msgid ""
"{e10b}{162f}{e10b}{1630}{e10b}{1631}{e10b}{1632}{e10b}{1633}{e10b}{1634}{e10b}{1635}{e10b}{1636}{e10b}{1637}{e10a}{1638}{e10b}{1639}{e10b}{163a}{e10b}{163b}{e10b}{163c}{e10b}{163d}{e10b}{163e}{e10b}{163f}{e10b}{1640}{e10b}{1641}{e10b}{1642}{e10b}{1643}{e10b}{1644}{e10b}{1645}{ec00}{0}\n"
"[DIALOG_START]{30}I'd really like to get someone\n"
"to go examine the\n"
"surveillance camera for me.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{e10b}{162f}{e10b}{1630}{e10b}{1631}{e10b}{1632}{e10b}{1633}{e10b}{1634}{e10b}{1635}{e10b}{1636}{e10b}{1637}{e10a}{1638}{e10b}{1639}{e10b}{163a}{e10b}{163b}{e10b}{163c}{e10b}{163d}{e10b}{163e}{e10b}{163f}{e10b}{1640}{e10b}{1641}{e10b}{1642}{e10b}{1643}{e10b}{1644}{e10b}{1645}{ec00}{0}\n"
"[DIALOG_START]{30}¿Es que nadie quiere\n"
"comprobar una cámara\n"
"de seguridad por mí?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "1"
msgid "[NULL]"
msgstr "[NULL]"

