msgid ""
"{e10b}{162f}{e10b}{1630}{e10b}{1631}{e10b}{1632}{e10b}{1633}{e10b}{1634}{e10b}{1635}{e10b}{1636}{e10b}{1637}{e10b}{1638}{e10b}{1639}{e10b}{163a}{e10b}{163b}{e10b}{163c}{e10b}{163d}{e10b}{163e}{e10b}{163f}{e10a}{1640}{e10b}{1641}{e10b}{1642}{e10b}{1643}{e10b}{1644}{e10b}{1645}{ec00}{0}\n"
"[DIALOG_START]{2e}I came all the way from the\n"
"countryside to see my little\n"
"brother.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{e10b}{162f}{e10b}{1630}{e10b}{1631}{e10b}{1632}{e10b}{1633}{e10b}{1634}{e10b}{1635}{e10b}{1636}{e10b}{1637}{e10b}{1638}{e10b}{1639}{e10b}{163a}{e10b}{163b}{e10b}{163c}{e10b}{163d}{e10b}{163e}{e10b}{163f}{e10a}{1640}{e10b}{1641}{e10b}{1642}{e10b}{1643}{e10b}{1644}{e10b}{1645}{ec00}{0}\n"
"[DIALOG_START]{2e}He venido del pueblo\n"
"para ver a mi hermano\n"
"pequeño.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "1"
msgid "[NULL]"
msgstr "[NULL]"

