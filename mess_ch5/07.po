msgctxt "0"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Prez]\n"
"Ahhh!\n"
"[NEW_DIALOG]\n"
"Wh-What are these things!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Prez]\n"
"W{e115}-We were just practicing\n"
"my speech for tomorrow's\n"
"election!\n"
"[NEW_DIALOG]\n"
"Do you have some kind of\n"
"problem!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Zack]\n"
"P-Prez...\n"
"[NEW_DIALOG]\n"
"I don't think we should really\n"
"be talking to viruses...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Prez]\n"
"Be quiet!\n"
"[NEW_DIALOG]\n"
"I already know that!\n"
"[NEW_DIALOG]\n"
"But what else is there for\n"
"us to do!?\n"
"[NEW_DIALOG]\n"
"...D{e115}-Don't worry about it!\n"
"[NEW_DIALOG]\n"
"A certain person will come.\n"
"He always does...!\n"
"[NEW_DIALOG]\n"
"He'll come...\n"
"[NEW_DIALOG]\n"
"Mega Man will come...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e100}<{ec00}{0}\n"
"[DIALOG_START][NONAME]\n"
"Preeez!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e213}{1}"
msgstr ""

msgctxt "2"
msgid ""
"{e212}{11}{1}{ec00}{0}\n"
"[DIALOG_START][Prez]\n"
"!!\n"
"[NEW_DIALOG]\n"
"S-See!!\n"
"He came...!!\n"
"[NEW_DIALOG]\n"
"Mega Man really came...!!\n"
"[NEW_DIALOG]\n"
"Mega Maaan!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "3"
msgid ""
"{e1a0}{4}{1e}{ec00}{0}\n"
"[DIALOG_START][Taurus_Fire]\n"
"Mmmrrrggg!\n"
"Preeez!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Prez]\n"
"{e116}...Huh?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Zack]\n"
"Th{e115}-That voice...!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Prez]\n"
"...B{e116}-Bud!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Zack]\n"
"B-B-Bud!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Taurus_Fire]\n"
"Is everyone OK!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "5"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Zack]\n"
"Geo and Sonia, too!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Prez]\n"
"Just what the heck is going\n"
"on here...!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Harp_Note]\n"
"We'll explain later!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Taurus_Fire]\n"
"We have to take care of\n"
"those viruses first!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Megaman]\n"
"Let's rock!\n"
"[NEW_DIALOG]\n"
"Wave Battle!{e115}\n"
"Ride On!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
