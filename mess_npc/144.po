msgctxt "10"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START]{74}Some Wizard went out of\n"
"control!\n"
"[NEW_DIALOG]\n"
"He must have gotten too\n"
"stressed out.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"I don't think that was it.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
"Me neither.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{74}...\n"
"[NEW_DIALOG]\n"
"*sniffle*\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START]{74}¡Un Wizard se ha salido\n"
"fuera de control!\n"
"[NEW_DIALOG]\n"
"Seguro que es por\n"
"el estrés.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"No creo que sea\n"
"eso.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
"Yo tampoco.\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START]{74}...\n"
"[NEW_DIALOG]\n"
"Snif.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "11"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START]{90}A Wizard went out of\n"
"control!\n"
"[NEW_DIALOG]\n"
"I wonder if it was an\n"
"electronic malfunction?\n"
"I have an enquiring mind.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START]{90}¡Un Wizard se ha salido\n"
"fuera de control!\n"
"[NEW_DIALOG]\n"
"¿Se le habrá averiado\n"
"algo? Tengo mucha\n"
"curiosidad.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

