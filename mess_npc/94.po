msgctxt "0"
msgid ""
"{e109}{0}{7a9}{7b1}{2}{e109}{0}{7a5}{7a9}{1}{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Hm-hm-hm!\n"
"[NEW_DIALOG]\n"
"The WAZA main computer is\n"
"the best in the world!\n"
"[NEW_DIALOG]\n"
"It's the handiwork of one\n"
"Dr. Goodall, the most revered\n"
"[NEW_DIALOG]\n"
"scientist of our time!\n"
"Hm-hm!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{e109}{0}{7a9}{7b1}{2}{e109}{0}{7a5}{7a9}{1}{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"¡Ajá, ajá, ajá!\n"
"[NEW_DIALOG]\n"
"¡El ordenador central de la\n"
"WAZA es el mejor del mundo!\n"
"[NEW_DIALOG]\n"
"Y es una obra del magnífico\n"
"Dr. Godall, ¡el mejor \n"
"[NEW_DIALOG]\n"
"científico de nuestros\n"
"tiempos!\n"
"¡Ajá, ajá!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Hm-hm!\n"
"[NEW_DIALOG]\n"
"Dark Phantom, one of the\n"
"most wanted criminals in\n"
"[NEW_DIALOG]\n"
"the world, has hacked into\n"
"the main computer!\n"
"[NEW_DIALOG]\n"
"Emergency!\n"
"Emergency! Hm-hm!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"¡Ajá, ajá!\n"
"[NEW_DIALOG]\n"
"¡Dark Phantom, uno de los\n"
"criminales más buscados\n"
"[NEW_DIALOG]\n"
"en el mundo, se ha metido\n"
"en el ordenador central!\n"
"[NEW_DIALOG]\n"
"¡Emergencia!\n"
"¡Emergencia! ¡Ajá, ajá!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "2"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Hm-hm!\n"
"[NEW_DIALOG]\n"
"Impressive.\n"
"Transcode 003\n"
"Mega Man! Hm-hm!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"¡Ajá, ajá!\n"
"[NEW_DIALOG]\n"
"Impresionante.\n"
"Transcode 003\n"
"¡Mega Man! ¡Ajá, ajá!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "3"
msgid ""
"{e109}{0}{7a9}{7b1}{5}{e109}{0}{7a5}{7a9}{4}{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Um-hm-hm...\n"
"[NEW_DIALOG]\n"
"3.14159265358979\n"
"3238462643383279\n"
"5028841971693...\n"
"[NEW_DIALOG]\n"
"It goes on forever...\n"
"pi... Um-hm-hm...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{e109}{0}{7a9}{7b1}{5}{e109}{0}{7a5}{7a9}{4}{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Ajá, ajá...\n"
"[NEW_DIALOG]\n"
"3.14159265358979\n"
"3238462643383279\n"
"5028841971693...\n"
"[NEW_DIALOG]\n"
"Pi nunca se acaba.\n"
"Ajá, ajá...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Um-hm-hm...\n"
"[NEW_DIALOG]\n"
"Dark Phantom went ahead\n"
"that way...\n"
"[NEW_DIALOG]\n"
"Danger... The computer's\n"
"hub is in there!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Ajá, ajá, ajá...\n"
"[NEW_DIALOG]\n"
"Dark Phantom se ha ido\n"
"por ahí.\n"
"[NEW_DIALOG]\n"
"¡Maldita sea!\n"
"¡Por ahí se va al concentrador\n"
"del ordenador!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "5"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Um-hm-hm...\n"
"[NEW_DIALOG]\n"
"Thank you...\n"
"The main computer\n"
"is safe.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Cibernúcleo]\n"
"Ajá, ajá, ajá...\n"
"[NEW_DIALOG]\n"
"Gracias...\n"
"El ordenador central\n"
"ya está a salvo.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

