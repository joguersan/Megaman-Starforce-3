msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Harry_Sasoon]\n"
"Please, cast your vote\n"
"for me!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Harry_Sasoon]\n"
"Por favor, ¡vótame!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "139"
