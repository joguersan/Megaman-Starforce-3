msgctxt "17"
msgid ""
"{e212}{1b}{1}{ec00}{0}\n"
"[DIALOG_START]{ad}{ec13}{e114}.{e114}.{e114}.{e114}\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Joker]\n"
"Hah...\n"
"[NEW_DIALOG]\n"
"Hah hah hah!\n"
"[NEW_DIALOG]\n"
"{e1a0}{4}{1e}\n"
"[DIALOG_START][Joker]\n"
"Hah hah hah!!\n"
"[NEW_DIALOG]\n"
