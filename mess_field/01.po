msgctxt "2"
msgid ""
"{e10c}{1}{22b}{5}{e10c}{1}{32d}{6}{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Mega]\n"
"Do you wanna pulse\n"
"out to {ec33}?\n"
"{ec05}{1}{2}   {ec06}Yes   {ec07}No\n"
"[NEW_DIALOG]\n"
"\n"
"{ec11}{0}{0}{ec11}{1}{1}{ec00}{1}"
msgstr ""
"{e10c}{1}{22b}{5}{e10c}{1}{32d}{6}{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Mega]\n"
"¿Quieres expulsionarte\n"
"a {ec33}?\n"
"{ec05}{1}{2}   {ec06}Sí   {ec07}No\n"
"[NEW_DIALOG]\n"
"\n"
"{ec11}{0}{0}{ec11}{1}{1}{ec00}{1}"

msgctxt "3"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"Looks like pulsing\n"
"out ain't an option\n"
"right now, kid!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"¡Chico, expulsionarse\n"
"ahora no es una\n"
"opción!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"You can't pulse outta\n"
"here with all this\n"
"Noise around.\n"
"[NEW_DIALOG]\n"
"Let's find an exit!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"No puedes expulsionarte\n"
"con todo este Ruido.\n"
"[NEW_DIALOG]\n"
"¡Vamos a buscar\n"
"una salida!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "5"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"The closest\n"
"Wave Station from\n"
"here is\n"
"[NEW_DIALOG]\n"
"in Sonia's dressing\n"
"room.\n"
"[NEW_DIALOG]\n"
"{ec17}{0}\n"
"[DIALOG_START][Mega]\n"
"Do ya wanna pulse out?\n"
"\n"
"{ec05}{1}{2}   {ec06}Yes   {ec07}No\n"
"[NEW_DIALOG]\n"
"\n"
"{ec11}{0}{0}{ec11}{1}{1}{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"La estación de ondas\n"
"más cercana está\n"
"[NEW_DIALOG]\n"
"en el camerino\n"
"de Sonia.\n"
"[NEW_DIALOG]\n"
"{ec17}{0}\n"
"[DIALOG_START][Mega]\n"
"¿Quieres expulsionarte?\n"
"\n"
"{ec05}{1}{2}   {ec06}Sí   {ec07}No\n"
"[NEW_DIALOG]\n"
"\n"
"{ec11}{0}{0}{ec11}{1}{1}{ec00}{1}"

msgctxt "6"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"It's no use!\n"
"The pulse out code\n"
"has been sealed!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e106}{1}"
msgstr ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"¡Es inútil!\n"
"¡Han bloqueado el\n"
"código de expulsión!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e106}{1}"
