"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"Ah,\n"
"«Felices vacaciones»...\n"
"[NEW_DIALOG]\n"
"{e212}!{0}\n"
"[DIALOG_START][Geo]\n"
"{ec13}{e116}.{e115}.{e115}.{e115}\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"{ec13}.{e115}.{e115}.{e115}\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Lyra]\n"
"{ec13}.{e115}.{e115}.{e115}\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
