msgctxt "0"
msgid ""
"{ec00}{2}\n"
"[DIALOG_START][Ace]\n"
"Welcome to the Satella Police\n"
"Network!\n"
"[NEW_DIALOG]\n"
msgstr ""
"{ec00}{2}\n"
"[DIALOG_START][Ace]\n"
"¡Bienvenido a la red\n"
"de la Policía Satella!\n"
"[NEW_DIALOG]\n"

msgctxt "1"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"You can battle your\n"
"Friends and make\n"
"Brothers!"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Puedes luchar contra\n"
"tus amigos y hacer\n"
"nuevos Hermanos!"

msgctxt "2"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Battle Friends and make\n"
"Brothers using Nintendo\n"
"Wi-Fi Connection."
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Para ello, debes usar\n"
"la conexión Wi-Fi de\n"
"Nintendo."

msgctxt "3"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"You can face off with\n"
"Friends!"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Enfréntate a tus\n"
"amigos!"

msgctxt "4"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"After a match, invite\n"
"your opponent to be\n"
"Brothers!"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Después de un combate,\n"
"ofrece a tu oponente\n"
"ser tu Hermano!"

msgctxt "5"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"After a match, invite\n"
"your opponent to be\n"
"your Battle Mate!"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Después de un combate,\n"
"ofrece a tu oponente\n"
"ser tu Colega!"

msgctxt "6"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"You can exchange\n"
"Battle Cards with\n"
"your Friends!"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Puedes intercambiar\n"
"cartas de combate\n"
"con tus amigos!"

msgctxt "7"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"You can exchange the\n"
"latest data with up to\n"
"6 Brothers!"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Puedes intercambiar los\n"
"últimos datos con hasta\n"
"seis Hermanos!"

msgctxt "8"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Disconnect from\n"
"the other party."
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Desconectar del\n"
"otro grupo."

msgctxt "9"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Do you want to disconnect?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¿Quieres desconectarte?\n"
"{ec10}{0}Sí\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"

msgctxt "10"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Disconnecting..."
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Desconectando..."

msgctxt "11"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Oops, it looks like the other\n"
"party disconnected.{e101}"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Ups, parece que el otro\n"
"grupo se ha desconectado.{e101}"

msgctxt "12"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Set up a session\n"
"as the Host and\n"
"let others connect."
msgstr ""

msgctxt "13"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Join a session\n"
"as a Client and\n"
"search for a Host."
msgstr ""

msgctxt "14"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Enter a 2 digit number\n"
"to find a specific\n"
"person."
msgstr ""

msgctxt "15"
msgid ""
"{ec00}{2}{ec17}{0}{ec18}\n"
"[DIALOG_START][Ace]\n"
"Waiting for a Client\n"
"to join..."
msgstr ""

msgctxt "16"
msgid ""
"{ec00}{2}{ec17}{0}{ec18}\n"
"[DIALOG_START][Ace]\n"
"Waiting for a Client to\n"
"join... (A Button: Stop\n"
"accepting clients)"
msgstr ""

msgctxt "17"
msgid ""
"{ec00}{2}{ec17}{0}{ec18}\n"
"[DIALOG_START][Ace]\n"
"Looking for a Host..."
msgstr ""

msgctxt "18"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Initiate a connection?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
msgstr ""

msgctxt "19"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"It looks like the Host\n"
"canceled the connection.\n"
"[NEW_DIALOG]\n"
"Do you want to search\n"
"again for a new host?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
msgstr ""

msgctxt "20"
msgid ""
"{ec00}{2}{ec17}{0}{ec18}\n"
"[DIALOG_START][Ace]\n"
"Negotiating with Host...\n"
"Hold on for a second."
msgstr ""

msgctxt "21"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"It looks like the Client\n"
"canceled the connection.\n"
"[NEW_DIALOG]\n"
"Do you want to restart\n"
"the session and wait for\n"
"a new Client to connect?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
msgstr ""

msgctxt "22"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"No Host was found.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "23"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"1 battle showdown\n"
"determines the winner.\n"
"Results are unaffected."
msgstr ""

msgctxt "24"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Best 2 out of 3\n"
"battles is the winner.\n"
"Results are unaffected."
msgstr ""

msgctxt "25"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"1 battle showdown\n"
