msgctxt "0"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Choose an ON AIR\n"
"Brother you want to battle.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "1"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Oops... It looks like that\n"
"Friend isn't connected to\n"
"Nintendo Wi-Fi Connection yet.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "2"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Do you want to invite\n"
"{e12c} to battle?\n"
"{ec10}{0}Invite\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"
msgstr ""

msgctxt "3"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"The invitation has been sent.\n"
"We're waiting for a reply..."
msgstr ""

msgctxt "4"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Do you want to cancel your\n"
"invitation to battle?\n"
"{ec10}{0}Cancel\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"
msgstr ""

msgctxt "5"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Sorry... It looks like they\n"
"turned down the invitation.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "6"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"{e12c} has\n"
"invited you to battle!\n"
"{ec10}{0}Accept\n"
"{ec10}{1}Decline\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"
msgstr ""

msgctxt "7"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"It looks like they\n"
"canceled on you.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "8"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"The other party either had\n"
"a communication error or\n"
"aren't responding.\n"
"[NEW_DIALOG]\n"
"It looks like we'll have to\n"
"cancel this time around.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "9"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"It looks like they're\n"
"tied up in a battle.\n"
"Let's try again later.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "10"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Declining invitation."
msgstr ""

msgctxt "11"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Battle has been\n"
"canceled.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "12"
msgid "{ec00}{1}"
msgstr ""

msgctxt "13"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Here are your results vs.\n"
"Battle Mate {e12c}.\n"
"{e121}{4} B & {e122}{4} W.\n"
"{ec10}{0}Del. Battle Mate\n"
"{ec10}{1}Go Back\n"
