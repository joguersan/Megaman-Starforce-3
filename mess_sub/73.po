msgctxt "0"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Your Purpose has changed!\n"
"Purpose Cards make\n"
"Mega Man even stronger!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Tu Propósito ha cambiado!\n"
"¡Con las Cartas Propósito,\n"
"Mega Man es aún más fuerte!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "1"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"This Purpose Card has\n"
"already been entered.\n"
"Update data?\n"
"[NEW_DIALOG]\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Esta Carta Propósito\n"
"ya se ha introducido.\n"
"¿Actualizar datos?\n"
"[NEW_DIALOG]\n"
"{ec10}{0}Sí\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"

msgctxt "2"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Install Purpose Card\n"
"Power Up Data!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Instalando datos de\n"
"mejora de Carta\n"
"Propósito!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "3"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"This customization was\n"
"already installed.\n"
"[NEW_DIALOG]\n"
"It has been overwritten\n"
"with the new one.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Ya has instalado esta\n"
"personalización.\n"
"[NEW_DIALOG]\n"
"Se ha sobreescrito\n"
"la nueva.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "4"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"When you form a Team, you\n"
"can share this customization\n"
"with your friends.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Cuando formas un Equipo,\n"
"puedes compartirla\n"
"con tus amigos.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "5"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"It looks like there was a\n"
"Purpose Card update!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Parece que la Carta\n"
"Propósito tiene una\n"
"actualización!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "6"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Starting Purpose Card\n"
"update!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Actualizando\n"
"Carta Propósito!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "7"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Sharing data with\n"
"{e12c}!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Compartiendo datos con\n"
"{e12c}!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "8"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Purpose Card update\n"
"has finished.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Actualización de\n"
"Carta Propósito\n"
"finalizada.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "9"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"{e12c}'s\n"
"shared data has been\n"
"deleted!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¡Se han borrado los\n"
"datos compartidos\n"
"de {e12c}!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

msgctxt "10"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Turn Purpose Card\n"
"Power Up ON?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¿Activar mejora de\n"
"la Carta Propósito?\n"
"{ec10}{0}Sí\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"

msgctxt "11"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Turn Purpose Card\n"
"Power Up OFF?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"¿Desactivar mejora de\n"
"la Carta Propósito?\n"
"{ec10}{0}Sí\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}{ec00}{1}"

msgctxt "12"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Purpose Card Power Up\n"
"has been turned ON.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Ace]\n"
"Mejora de la Carta\n"
"Propósito activada.\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"

