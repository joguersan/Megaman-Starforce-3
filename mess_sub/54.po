"{ec0f}{0}{0}{2}{0}"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"¿Quieres registrar\n"
"el Ruido enemigo?\n"
"{ec10}{0}Sí\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"

msgctxt "10"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Do you want to set this\n"
"\"{e588}\"?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
