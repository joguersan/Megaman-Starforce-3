msgctxt "1"
msgid ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Do you want to use\n"
"Sub Card: \"{e12f}\"?\n"
"{ec10}{0}Yes\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"
msgstr ""
"{ec00}{0}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"¿Quieres usar la\n"
"Subcarta: «{e12f}»?\n"
"{ec10}{0}Sí\n"
"{ec10}{1}No\n"
"{ec0f}{0}{0}{2}{0}"

msgctxt "2"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"This card is used\n"
"automatically."
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Esta carta se usa\n"
"automáticamente."

msgctxt "3"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"You just used that one.\n"
"Let's hold off on it\n"
"for now."
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Pero si acabas de usarlo.\n"
"Dale un respiro."

msgctxt "4"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Let's not use this\n"
"just now."
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Será mejor que no\n"
"usemos esto ahora."

msgctxt "5"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"MegaMan recovered\n"
"{e123}{4} HP!\n"
"[NEW_DIALOG]\n"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"¡Mega Man ha recuperado\n"
"{e123}{4} PV!\n"
"[NEW_DIALOG]\n"

msgctxt "6"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"MegaMan recovered\n"
"all of his HP!\n"
"[NEW_DIALOG]\n"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"¡Mega Man ha recuperado\n"
"todos sus PV!\n"
"[NEW_DIALOG]\n"

msgctxt "7"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"MegaMan has disappeared!\n"
"Weaker foes won't be able\n"
"to find him for a while!\n"
"[NEW_DIALOG]\n"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"¡Mega Man ha desaparecido!\n"
"¡Los enemigos débiles no le\n"
"verán en un rato!\n"
"[NEW_DIALOG]\n"

msgctxt "8"
msgid ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"Mega's eyes twinkled!\n"
"It's very likely he'll meet\n"
"foes that he met before.\n"
"[NEW_DIALOG]\n"
msgstr ""
"{ec00}{2}{ec17}{0}\n"
"[DIALOG_START][Geo]\n"
"¡A Mega le brillan los ojos!\n"
"Aparecerán enemigos\n"
"ya derrotados.\n"
"[NEW_DIALOG]\n"

