msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Prez]\n"
"Geo...\n"
"[NEW_DIALOG]\n"
"Sonia?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Bud]\n"
"Preeez!!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Zack]\n"
"P-P-Preeez!!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
"{ec00}{0}\n"
