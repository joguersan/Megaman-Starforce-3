msgctxt "0"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo_2]\n"
"{ec13}...\n"
"[NEW_DIALOG]\n"
"(I was just here\n"
"{ec13} with Prez, recently...)\n"
"{ec13}Prez...\n"
"[NEW_DIALOG]\n"
"{ec13}...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e1a2}{0}{0}<{e100}<{ec00}{0}\n"
"[DIALOG_START][Prez]\n"
"Hey, Geo!\n"
"[NEW_DIALOG]\n"
"This isn't the time to be\n"
"depressed!\n"
"[NEW_DIALOG]\n"
"Hurry up and turn into\n"
"Mega Man and save me!\n"
"[NEW_DIALOG]\n"
"OK!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}{e1a3}{0}<{e100}<{ec00}{0}\n"
"[DIALOG_START][Geo]\n"
"{ec13}(...Haha! That's\n"
"{ec13} just like Prez...!)\n"
"[NEW_DIALOG]\n"
"Alright!\n"
"It's time to find Mr. Boreal!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"{e115}...Wait a second!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "1"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo_2]\n"
"Is something wrong, Mega!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"It feels like we're being\n"
"watched...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo_2]\n"
"Really!?\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "2"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"You idiot!\n"
"Don't look for them!\n"
"[NEW_DIALOG]\n"
"They'll see you and...!\n"
"[NEW_DIALOG]\n"
"Huh!?\n"
"I think they went away!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "3"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Geo_2]\n"
"I-I'm sorry, Mega...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"Come on!\n"
"You need to be more\n"
"cautious at times like this!\n"
"[NEW_DIALOG]\n"
"You can't always be a goofy\n"
"kid!\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo_2]\n"
"I-I know!\n"
"[NEW_DIALOG]\n"
"But...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"What?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo_2]\n"
"...It's just that I never\n"
"expected you to yell at me\n"
"to be more cautious...\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Mega]\n"
"...Hey!\n"
"What does that mean!?\n"
"[NEW_DIALOG]\n"
"\n"
"[DIALOG_START][Geo]\n"
"N-Nothing!\n"
"Nothing at all!\n"
"[NEW_DIALOG]\n"
"We should get going and\n"
"find Mr. Boreal!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "4"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][Mega]\n"
"Don't think you can fool me!\n"
"[NEW_DIALOG]\n"
"Hey!\n"
"Wait up, kid!\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""

msgctxt "5"
msgid ""
"{ec00}{0}\n"
"[DIALOG_START][NONAME]\n"
"Geo Stelar{e114}.{e114}.{e114}.{e114}\n"
"No... Mega Man!\n"
"[NEW_DIALOG]\n"
"Your power will be mine...\n"
"[NEW_DIALOG]\n"
"{ec00}{1}"
msgstr ""
